<div class="col-sm-4">
    <h5><%= citywalk.name %></h5> 
    <%= link_to "See more", citywalk_path(citywalk), class: "btn btn-primary" %>

    <% if has_role?(:admin) %>
	    <div class="btn-group">
	        <%= link_to "Edit", edit_citywalk_path(citywalk), class: "btn btn-secondary" %>
	        <%= link_to "Delete", citywalk_path(citywalk), method: :delete, data: { confirm: "Are you sure?" }, class: "btn btn-danger" %>
	    </div>
	<% end %>
</div>