<div class="col-md-6 offset-3">

    <div class="edit-country-form">

        <h1>Edit country</h1>

        <%= simple_form_for @country, :html => { :multipart => true } do |f| %>
            <%= f.input :name, label: "Country name" %>
            <%= f.input :description %>
            <%= f.button :submit %>
        <% end %>

    </div>

</div>